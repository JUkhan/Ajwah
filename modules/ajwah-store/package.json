    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.5.4",
    "rxjs": "^6.4.0"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0-beta.0 <7"
  },
  "repository": {
    "type": "git",
