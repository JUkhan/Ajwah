{
  "name": "ajwah-store",
  "version": "4.2.3",
  "description": "A reactive state management library. Manage your application's states, effects, and actions easy way. Make apps more scalable with a unidirectional data-flow.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.cjs.spec.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.esm5.rollup.json ./src/tsconfig.types.json",
    "build:clean": "shx rm -rf ./dist",
    "build:global": "node ./tools/make-umd-bundle.js && node ./tools/make-closure-core.js",
    "build:package": "npm-run-all build:clean compile build:global && node ./tools/prepare-package.js && node ./tools/generate-alias.js",
    "rollup":"rollup ./dist/esm5_for_rollup/internal/umd.js --format umd --name myBundle --file bundle.js"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.4.6",
    "@angular-devkit/schematics": "^7.3.9",
    "@types/lodash": "4.14.102",
    "@types/node": "^14.14.6",
    "@types/shelljs": "^0.8.8",
    "@types/sinon": "4.1.3",
    "@types/sinon-chai": "2.7.29",
    "@types/source-map": "^0.5.2",
    "babel-polyfill": "6.26.0",
    "check-side-effects": "0.0.23",
    "color": "3.0.0",
    "colors": "1.1.2",
    "cross-env": "5.1.3",
    "cz-conventional-changelog": "1.2.0",
    "dependency-cruiser": "^9.12.0",
    "escape-string-regexp": "1.0.5",
    "eslint": "^7.8.1",
    "eslint-plugin-jasmine": "^2.10.1",
    "form-data": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "7.1.2",
    "google-closure-compiler-js": "20170218.0.0",
    "husky": "^4.2.5",
    "klaw-sync": "3.0.2",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "4.1.2",
    "opn-cli": "3.1.0",
    "platform": "1.3.5",
    "prettier": "^2.0.5",
    "promise": "8.0.1",
    "rollup": "2.26.0",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-inject": "3.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rxjs": "^6.0.0",
    "shelljs": "^0.8.4",
    "shx": "^0.3.2",
    "sinon": "4.3.0",
    "sinon-chai": "2.14.0",
    "source-map-support": "0.5.3",
    "systemjs": "^0.21.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "validate-commit-msg": "2.14.0",
    "webpack": "^4.31.0"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0-beta.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JUkhan/Ajwah.git"
  },
  "keywords": [
    "ajwah",
    "rxjs",
    "reactive",
    "redux"
  ],
  "files": [
    "dist/bundles",
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "dist/esm5/**/!(*.tsbuildinfo)",
    "dist/types/**/!(*.tsbuildinfo)",
    "src",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "author": "Jasim Uddin Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JUkhan/Ajwah/issues"
  },
  "homepage": "https://github.com/JUkhan/Ajwah#readme"
}
